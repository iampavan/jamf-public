				<key>file_content</key>
				<string>#!/bin/bash

xcode_name="%NAME%"
xcode_version="%version%"
xcode_name_and_version="${xcode_name}_${xcode_version}.app"

# Ensure everyone is a member of "developer" group
/usr/sbin/dseditgroup -o edit -a everyone -t group _developer

# Enable Developer Mode
/usr/sbin/DevToolsSecurity -enable

# Accept the license
/Applications/${xcode_name_and_version}/Contents/Developer/usr/bin/xcodebuild -license accept

# Install embedded packages
for PKG in `/bin/ls /Applications/${xcode_name_and_version}/Contents/Resources/Packages/*.pkg` ; do
  /usr/sbin/installer -pkg "$PKG" -target /
done

exit 0</string>
